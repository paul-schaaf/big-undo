{
    "name": "big-undo",
    "displayName": "big undo",
    "publisher": "PaulSchaaf",
    "repository": {
        "type": "git",
        "url": "https://github.com/paul-schaaf/big-undo"
    },
    "description": "resets file to earlier state",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.70.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "configuration": {
            "title": "big-undo",
            "properties": {
                "big-undo.maximumSnapshotsSaved": {
                    "type": "number",
                    "default": 10,
                    "description": "How many snapshots the extension will save of the currently active file"
                }
            },
            "commands": [
                {
                    "command": "big-undo.undo",
                    "title": "Undo"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.70.0",
        "@types/mocha": "^10.0.3",
        "@types/node": "18.x",
        "@typescript-eslint/eslint-plugin": "^6.9.0",
        "@typescript-eslint/parser": "^6.9.0",
        "eslint": "^8.52.0",
        "glob": "^10.3.10",
        "mocha": "^10.2.0",
        "typescript": "^5.2.2",
        "@vscode/test-electron": "^2.3.6",
        "prettier": "2.8.7"
    }
}
